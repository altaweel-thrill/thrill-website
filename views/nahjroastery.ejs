  color: #fff;
  opacity: 1;                 /* ğŸ‘ˆ Ø¸Ø§Ù‡Ø±Ø© */
  transition: opacity .25s ease;
  pointer-events: none;
}

/* âœ… CSS HOVER ONLY (NO JS) */
.reel-card:hover .overlay {
  opacity: 1;
  visibility: visible;
}

.reel-card:hover img,
.reel-card:hover video {
  filter: brightness(0.55);
  transform: scale(1.05);
}

/* Text */
.overlay h4 {
  margin: 0;
  font-size: 16px;
}

.overlay p {
  margin: 0;
  font-size: 13px;
  color: #ccc;
}

/* Tag */
.tag {
  font-size: 11px;
  padding: 4px 12px;
  border-radius: 100px;
  background: #ff0050;
  width: fit-content;
}

