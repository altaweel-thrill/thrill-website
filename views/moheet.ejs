    /* remove theme lines/borders/padding in gallery area */
    .portfolio__detail-content .container.g-0.line {
      max-width: 100%;
      padding-left: 0;
      padding-right: 0;
      border: 0;
    }

    .portfolio__detail-top .container.g-0.line {
      border: 0;
    }

    .portfolio__detail-content .line-3,
    .portfolio__detail-top .line-3 {
      display: none;
    }

    /* main gallery wrapper */
    .behance-gallery {
      max-width: 1400px;
      margin: 0 auto;
      padding: 0 24px 120px;
    }

    .behance-block,
    .behance-block-wide {
      margin-bottom: 80px;
    }

    .behance-block img,
    .behance-block-wide img {
      width: 100%;
      height: auto;
      border-radius: 24px;
      display: block;
      box-shadow: 0 30px 80px rgba(0, 0, 0, 0.4);
      background-color: #0f0f0f;
      will-change: transform, opacity;
    }

    /* two-column sections */
    .behance-row-2 {
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 40px;
      margin-bottom: 80px;
      padding: 0;
    }

    .behance-col img {
      width: 100%;
      height: auto;
      border-radius: 16px;
      display: block;
      box-shadow: 0 30px 80px rgba(0, 0, 0, 0.4);
      background-color: #0f0f0f;
      will-change: transform, opacity;
    }

    /* buttons section tweaks */
    .portfolio__detail-btns {
      display: flex;
      justify-content: space-between;
      align-items: center;
      border-top: 1px solid rgba(255, 255, 255, 0.08);
      border-bottom: 1px solid rgba(255, 255, 255, 0.08);
      padding-top: 60px;
      padding-bottom: 60px;
      margin-top: 40px;
    }

    .portfolio__detail-btns a.wc-btn-primary {
      background: transparent;
      border: 1px solid rgba(255, 255, 255, 0.2);
      border-radius: 100px;
      padding: 14px 28px;
      font-size: 15px;
      line-height: 1.2;
      color: #fff;
      display: inline-flex;
      align-items: center;
      gap: 8px;
      position: relative;
      overflow: hidden;
    }

    .portfolio__detail-btns a.wc-btn-primary span {
      display: none;
    }

    /* CTA block bottom section */
    .cta__area {
      background-color: #0a0a0a;
    }

    .cta__sub-title {
      color: #888;
      font-size: 0.9rem;
      text-transform: uppercase;
      letter-spacing: 0.08em;
      margin-bottom: 12px;
    }

    .cta__title {
      color: #fff;
      font-weight: 600;
      line-height: 1.2;
      font-size: clamp(2rem, 0.5vw + 1.5rem, 2.6rem);
      max-width: 700px;
    }

    .btn_wrapper .wc-btn-primary {
      background: #fff;
      color: #000;
      border-radius: 100px;
      border: 0;
      padding: 14px 24px;
      line-height: 1.2;
      font-size: 15px;
      font-weight: 500;
      margin-top: 28px;
      display: inline-flex;
      align-items: center;
      gap: 8px;
    }

    .btn_wrapper .wc-btn-primary i {
      font-size: 14px;
    }

    /* responsive */
    @media (max-width: 991px) {

      .behance-gallery {
        padding: 0 16px 80px;
      }

      .behance-row-2 {
        grid-template-columns: 1fr;
        gap: 24px;
        margin-bottom: 60px;
      }

      .behance-block,
      .behance-block-wide {
        margin-bottom: 60px;
      }

      .behance-block img,
      .behance-block-wide img,
      .behance-col img {
        border-radius: 14px;
        box-shadow: 0 20px 50px rgba(0, 0, 0, 0.6);
      }

      .portfolio__detail-btns {
        flex-direction: column;
        gap: 24px;
        text-align: center;
      }
    }
  </style>

