  <!-- ✅ GSAP gallery reveal / parallax -->
  <script>
    // register plugin (احتياط لو ما تسجل قبل)
    if (typeof gsap !== "undefined" && typeof ScrollTrigger !== "undefined") {
      gsap.registerPlugin(ScrollTrigger);
    }

    // كل الصور في المعرض
    const galleryImages = document.querySelectorAll(
      ".behance-block img, .behance-block-wide img, .behance-col img"
    );

    galleryImages.forEach((img) => {
      // نخفي وننزل الصورة بالبداية
      gsap.set(img, {
        autoAlpha: 0,
        y: 40,
      });

      // وندخلها بسكرول
      gsap.to(img, {
        autoAlpha: 1,
        y: 0,
        duration: 1.1,
        ease: "power3.out",
        scrollTrigger: {
          trigger: img,
          start: "top 80%", // اول ما الصورة تدخل تحت الهيدر شوي
          toggleActions: "play none none none",
          once: true, // مره وحده فقط
        },
      });
    });

    // بارالاكس خفيف للصور الواسعة
    const parallaxBlocks = document.querySelectorAll(
      ".behance-block-wide img, .behance-block img"
    );

    parallaxBlocks.forEach((img) => {
      gsap.to(img, {
        yPercent: -5,
        ease: "none",
        scrollTrigger: {
          trigger: img,
          start: "top bottom",
          end: "bottom top",
          scrub: 1,
        },
      });
    });
  </script>

