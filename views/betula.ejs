/* ================= TITLE ================= */
.page-header {
  padding: 140px 0 80px;
  text-align: center;
}

.page-header h1 {
  font-size: clamp(2.5rem, 4vw, 4rem);
  font-weight: 600;
  margin: 0;
}

.page-header p {
  margin-top: 14px;
  font-size: 16px;
  color: #9f9f9f;
}

/* ================= SECTION ================= */
.reels-portfolio {
  padding-bottom: 120px;
}

.container {
  max-width: 1400px;
  margin: auto;
  padding: 0 24px;
}

/* ================= GRID ================= */
.reels-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 28px;
}

/* ================= CARD ================= */
.reel-card {
  position: relative;
  border-radius: 22px;
  overflow: hidden;
  background: #000;
  cursor: pointer;
}

/* 9:16 */
.reel-card .media {
  aspect-ratio: 4 / 5 ;
  overflow: hidden;
}

.reel-card .media2 {
  aspect-ratio: 9 / 16 ;
  overflow: hidden;
}


.reel-card img,
.reel-card video {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform .5s ease, filter .4s ease;
}

/* Overlay */
.reel-card .overlay {
  position: absolute;
  inset: 0;
  padding: 22px;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  gap: 8px;
  opacity: 0;
  visibility: hidden;
  transition: opacity .35s ease;
}

.reel-card.video:hover .play-icon {
  opacity: 0;
}

.play-icon {
  position: absolute;
  inset: 0;
  z-index: 3;                 /* ğŸ‘ˆ Ø£Ø¹Ù„Ù‰ Ù…Ù† Ø§Ù„ÙÙŠØ¯ÙŠÙˆ */
